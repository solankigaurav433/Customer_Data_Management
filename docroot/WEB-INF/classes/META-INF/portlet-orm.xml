<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.docs.model.impl.CustomerModelImpl">
		<attributes>
			<id name="customerId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="name" />
			<basic name="email" />
			<basic name="address" />
			<basic name="phone" />
			<basic name="passportNo" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="serviceId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.docs.model.impl.ServicesModelImpl">
		<attributes>
			<id name="serviceId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="name" />
			<basic name="description" />
			<basic name="price" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.docs.model.impl.CustomerImpl" name="Customer">
		<table name="AA_Customer" />
		<attributes />
	</entity>
	<entity class="com.liferay.docs.model.impl.ServicesImpl" name="Services">
		<table name="AA_Services" />
		<attributes />
	</entity>
</entity-mappings>